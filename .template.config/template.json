{
    "$schema": "https://json.schemastore.org/template",
    "author": "happyfr1st",
    "classifications": ["Web", "WebApi", "Api", "SPA"],
    "name": "ASP Net Core 8 Web Api Repository Pattern with Unit of Work",
    "description": "A template for creating an ASP.NET Core Web Api with the repository- and unit of work pattern",
    "identity": "RepositoryTemplate",
    "shortName": "repositorytemplate",
    "sourceName": "RepositoryTemplate",
    "tags": {
      "language": "C#",
      "type": "project"
    },
    "preferNameDirectory": true,
    "symbols": {
      "Framework": {
        "type": "parameter",
        "description": "The target framework for the project.",
        "datatype": "choice",
        "choices": [
          {
            "choice": "net8.0"
          }
        ],
        "defaultValue": "net8.0",
        "replaces": "{TargetFramework}"
      }
    }
  }